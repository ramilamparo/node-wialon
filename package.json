{
	"name": "node-wialon",
	"version": "1.16.0",
	"description": "A NodeJS wrapper implementation for Wialon Remote API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"typings": "dist/index.d.ts",
	"scripts": {
		"lint": "eslint --fix --ext .ts,.tsx src --color --config .eslintrc",
		"format": "prettier --write \"src/**/*.ts\"",
		"build": "tsc",
		"preversion": "npm run format && npm run lint && npm run build",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ramilamparo/node-wialon.git"
	},
	"keywords": [
		"wialon"
	],
	"author": "Ramil Amparo",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ramilamparo/node-wialon/issues"
	},
	"homepage": "https://github.com/ramilamparo/node-wialon#readme",
	"devDependencies": {
		"@types/nock": "^11.1.0",
		"@types/node": "^14.14.35",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"eslint": "^7.22.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"nock": "^13.0.11",
		"prettier": "^2.2.1",
		"prettier-eslint": "^12.0.0",
		"prettier-eslint-cli": "^5.0.1",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"form-data": "^4.0.0"
	},
	"files": [
		"dist/**/*"
	]
}
